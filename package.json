{
  "name": "api-rest-node-auth",
  "version": "1.0.0",
  "description": "Authentication Microservice",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./",
    "build": "tsc -p .",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "knex:migrate": "knex --knexfile ./src/database/knex/Environment.ts migrate:latest",
    "knex:rollback": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback",
    "knex:rollback-all:": "knex --knexfile ./src/database/knex/Environment.ts migrate:rollback --all"
  },
  "repository": {
    "type": "git",
    "url": "api-rest-node-auth"
  },
  "author": "nozix",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.0",
    "sqlite3": "^5.1.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/knex": "^0.16.1",
    "axios": "^1.5.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "knex": "^2.5.1",
    "mqtt": "^5.1.2",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2"
  }
}
